{
    "$schema": "https://json.schemastore.org/package.json",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build && node move-manifest.js",
        "preview": "vite preview",
        "serve": "php artisan serve",
        "dev:all": "concurrently \"npm run serve\" \"npm run dev\"",
        "dev:app": "concurrently \"php artisan serve\" \"npm run dev -- --open\"",
        "offline": "concurrently \"php artisan serve\" \"npm run dev\"",
        "offline:sync": "concurrently \"php artisan loans:sync-status && php artisan serve\" \"npm run dev\""
    },
    "devDependencies": {
        "@headlessui/react": "^2.0.0",
        "@inertiajs/react": "^2.0.0",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/vite": "^4.0.0",
        "@vitejs/plugin-react": "^4.7.0",
        "autoprefixer": "^10.4.12",
        "axios": "^1.11.0",
        "concurrently": "^9.2.1",
        "laravel-vite-plugin": "^2.0.0",
        "postcss": "^8.4.31",
        "prettier": "^3.6.2",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "tailwindcss": "^3.2.1",
        "vite": "^7.1.7"
    },
    "dependencies": {
        "chart.js": "^4.5.0",
        "html2pdf.js": "^0.12.1",
        "nprogress": "^0.2.0",
        "react-chartjs-2": "^5.3.0",
        "react-hot-toast": "^2.6.0",
        "react-icons": "^5.5.0"
    }
}
